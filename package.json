    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "0.7.1",
    "gulp-tap": "^0.1.1",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "handlebars-helpers": "^0.5.6",
    "partialify": "^3.1.1",
    "pretty-hrtime": "~0.2.1",
